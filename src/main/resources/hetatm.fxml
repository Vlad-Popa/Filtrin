<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.control.*?>
<TableView prefHeight="360.0" prefWidth="480.0"
           editable="false"
           xmlns="http://javafx.com/javafx/8.0.40"
           xmlns:fx="http://javafx.com/fxml/1">
  <columns>
      <TableColumn text="Identifier" >
        <cellValueFactory>
            <PropertyValueFactory property="pdb" />
        </cellValueFactory>
      </TableColumn>
      <TableColumn text="Min" >
          <cellValueFactory>
              <PropertyValueFactory property="min" />
          </cellValueFactory>
      </TableColumn>
      <TableColumn text="Max" >
          <cellValueFactory>
              <PropertyValueFactory property="max" />
          </cellValueFactory>
      </TableColumn>
      <TableColumn text="Mean" >
          <cellValueFactory>
              <PropertyValueFactory property="avg" />
          </cellValueFactory>
      </TableColumn>
      <TableColumn text="St. Deviation" >
          <cellValueFactory>
              <PropertyValueFactory property="std" />
          </cellValueFactory>
      </TableColumn>
  </columns>
    <columnResizePolicy>
        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
    </columnResizePolicy>
</TableView>
